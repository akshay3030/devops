{
  "checks": [
  {
    "id": "cpuCheck",
    "notes": "CPU: Warning >CNSLCWARN% , Critical >CNSLCCRIT%",
    "name": "CPU Check",
    "args": ["/usr/local/bin/syschecks.sh", "-p", "-w", "CNSLCWARN", "-c", "CNSLCCRIT"],
    "interval": "10s",
    "timeout": "10s"
  },
  {
    "id": "diskCheck",
    "notes": "Disk: Warning >CNSLDWARN% , Critical >CNSLDCRIT%",
    "name": "Disk Check",
    "args": ["/usr/local/bin/syschecks.sh", "-d", "-w", "CNSLDWARN", "-c", "CNSLDCRIT"],
    "interval": "10s",
    "timeout": "10s"
  }
  ]
}
